<Window x:Class="ProcessComunication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessComunication"
        mc:Ignorable="d"
        Title="MainWindow" Height="170.17" Width="557.991">
    <Grid>
        <TextBox x:Name="tbSend" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="tblRead" HorizontalAlignment="Left" Margin="374,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="btSend" Content="Escribir" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" Width="75" Click="btSend_Click"/>
        <Button x:Name="btRead" Content="Leer" HorizontalAlignment="Left" Margin="374,55,0,0" VerticalAlignment="Top" Width="75" Click="btRead_Click"/>
        <Button x:Name="btCreate" Content="Crear nuevo proceso" HorizontalAlignment="Left" Margin="402,95,0,0" VerticalAlignment="Top" Width="127" Click="btCreate_Click"/>

    </Grid>
</Window>
